{
  "name": "proteus-author",
  "version": "0.1.0",
  "description": "A minimal Electron + Vue application",
  "main": "build/main/main.js",
  "scripts": {
    "dev": "node scripts/dev-server.js",
    "build": "node scripts/build.js && electron-builder -c config/electron-builder.json",
    "build:win": "node scripts/build.js && electron-builder -c config/electron-builder.json --win",
    "build:mac": "node scripts/build.js && electron-builder -c config/electron-builder.json --mac",
    "build:linux": "node scripts/build.js && electron-builder -c config/electron-builder.json --linux",
    "lint": "eslint src/renderer/**/*.{ts,vue} && eslint src/main/**/*.ts",
    "lint:fix": "eslint --fix src/renderer/**/*.{ts,vue} && eslint --fix src/main/**/*.ts"
  },
  "repository": "https://github.com/deluze/electron-vue-template",
  "author": {
    "name": "Adam Howard",
    "url": "https://github.com/howardah"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/lodash": "^4.14.184",
    "@types/node": "^18.7.13",
    "@types/p5": "^1.4.2",
    "@types/pizzicato": "^0.6.0",
    "@types/uuid": "^8.3.4",
    "@types/wavesurfer.js": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "@vitejs/plugin-vue": "^2.2.4",
    "@vue/cli": "^5.0.1",
    "@vue/eslint-config-typescript": "^11.0.1",
    "chalk": "^4.1.2",
    "chokidar": "^3.5.3",
    "electron": "^20.1.0",
    "electron-builder": "^23.3.3",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^22.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-vue": "^9.4.0",
    "prettier": "^2.7.1",
    "sass": "^1.54.5",
    "typescript": "*",
    "vite": "^3.0.9"
  },
  "dependencies": {
    "electron-is-dev": "^2.0.0",
    "element-plus": "^2.2.15",
